{
  "expo": {
    "name": "gymify",
    "slug": "gymify",
    "version": "1.0.0",
    "icon" : "./assets/images/icon.png",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",

   "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "cover",
      "backgroundColor": "#1F1F1F"
  },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shefali.gymify",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
      
    },
    "android": {
      "package": "com.shefali.gymify",
       "splash": {
    "backgroundColor": "#1F1F1F"
  },
       "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
          "backgroundColor": "#1F1F1F"

      }
     
    },
    "web": {
      "bundler": "metro",
      "output": "static"
      
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      "expo-font",
      "expo-web-browser"
    
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0a8eae0c-2341-44cd-a769-9bfc88d1fc0b"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/0a8eae0c-2341-44cd-a769-9bfc88d1fc0b"
    },
    "owner": "shefali_choudhary"
  }
}
