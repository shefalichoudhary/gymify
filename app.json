{
  "expo": {
    "name": "gymify",
    "slug": "gymify",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

  
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shefali.gymify",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "splash": {
    "backgroundColor": "#1F1F1F",
    "image": "./assets/images/logo.png",
    "resizeMode": "contain"
  }
    },
    "android": {
      "package": "com.shefali_choudhary.gymify",
      "splash" : null
      },
    
    "web": {
      "bundler": "metro",
      "output": "static",
      "splash": {
        "backgroundColor": "#1F1F1F",
        "resizeMode": "contain"
      }
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "867635d0-876f-4757-a778-75594b3b2532"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/867635d0-876f-4757-a778-75594b3b2532"
    },
    "owner": "shefali_choudhary"
  }
}
